@inproceedings{kcc21daf,
    abbr={KCC 2021},
    author={Seongwhan Min and Wonseok Shin and Chaewon Kim and Kunsoo Park},
    title={부분 그래프 매칭 문제를 위한 향상된 동적 매칭 순서},
    booktitle={한국정보과학회 학술발표논문집},
    publisher={한국정보과학회},
    year={2021},
