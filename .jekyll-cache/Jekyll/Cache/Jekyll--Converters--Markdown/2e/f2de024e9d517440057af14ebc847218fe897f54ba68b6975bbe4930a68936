            <span class="p">}</span> <span class="k">while</span> <span class="p">((</span><span class="n">idx</span> <span class="o">&lt;</span> <span class="n">mid</span><span class="p">)</span> <span class="o">==</span> <span class="p">(</span><span class="n">next</span><span class="o">-&gt;</span><span class="n">l</span> <span class="o">&lt;</span> <span class="n">mid</span><span class="p">));</span>
            <span class="n">Node</span> <span class="o">*</span> <span class="n">nnode</span> <span class="o">=</span> <span class="k">new</span> <span class="n">Node</span><span class="p">(</span><span class="n">lo</span><span class="p">,</span> <span class="n">hi</span><span class="p">);</span>
