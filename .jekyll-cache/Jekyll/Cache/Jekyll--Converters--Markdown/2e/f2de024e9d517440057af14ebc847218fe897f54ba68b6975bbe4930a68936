        <span class="k">else</span>
        <span class="p">{</span>
            <span class="k">do</span> <span class="p">{</span>
                <span class="p">(</span><span class="n">idx</span> <span class="o">&lt;</span> <span class="n">mid</span> <span class="o">?</span> <span class="n">hi</span> <span class="o">:</span> <span class="n">lo</span><span class="p">)</span> <span class="o">=</span> <span class="n">mid</span><span class="p">;</span>
