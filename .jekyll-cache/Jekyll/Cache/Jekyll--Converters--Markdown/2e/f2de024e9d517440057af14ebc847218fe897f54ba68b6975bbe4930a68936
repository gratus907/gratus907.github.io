    <span class="k">if</span> <span class="p">(</span><span class="n">m</span> <span class="o">==</span> <span class="mi">2</span><span class="p">)</span>
        <span class="n">solve_2</span><span class="p">();</span>
    <span class="k">else</span> <span class="n">solve_3</span><span class="p">();</span>
<span class="p">}</span>


